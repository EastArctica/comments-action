{
  "name": "comments-action",
  "version": "1.0.8",
  "description": "GitHub Action for posting commit comments in the Discord-Datamining repo",
  "type": "commonjs",
  "main": "dist/index.js",
  "scripts": {
    "build": "ncc build src/index.ts -o out --minify && tsc out/index.js --outDir dist --allowJs --sourceMap false",
    "commit": "pnpm run build && git add -A && git commit -m 'Update'",
    "postcommit": "git push",
    "test": "tsc && node ./dist/test/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Discord-Datamining/comments-action.git"
  },
  "author": "Tiemen",
  "license": "MIT",
  "homepage": "https://github.com/Discord-Datamining/comments-action#readme",
  "dependencies": {
    "@actions/core": "^1.9.1",
    "@actions/github": "^5.1.0",
    "@adryd325/discord-datamining-lang-differ": "github:adryd325/discord-datamining-lang-differ#ec426f9422b7b85a9dac45a8bfd7a6926c0b9474",
    "@types/he": "^1.2.3",
    "@types/js-beautify": "^1.14.3",
    "ansi-to": "^1.5.1",
    "ansi-to-svg": "^1.4.3",
    "cssjson": "^2.1.3",
    "diff": "^5.1.0",
    "form-data": "^4.0.0",
    "he": "^1.2.0",
    "js-beautify": "^1.14.11",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "@octokit/webhooks-types": "^6.3.6",
    "@types/diff": "^5.0.8",
    "@types/node": "^18.7.23",
    "@vercel/ncc": "0.34.0",
    "typescript": "^4.8.3"
  }
}